{
  "name": "vue-document-editor",
  "version": "1.0.0",
  "description": "A Vue 3 Document Layout Editor - drag and drop interface for creating professional documents",
  "main": "dist/vue-document-editor.cjs.js",
  "module": "dist/vue-document-editor.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --mode lib",
    "preview": "vite preview"
  },
  "exports": {
    ".": {
      "import": "./dist/vue-document-editor.es.js",
      "require": "./dist/vue-document-editor.cjs.js"
    },
    "./dist/style.css": "./dist/vue-document-editor.css"
  },
  "keywords": [
    "vue",
    "vue3",
    "document",
    "editor",
    "layout",
    "drag-drop",
    "template",
    "visual-editor",
    "wysiwyg",
    "form-builder"
  ],
  "author": "Inayat Ullah",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@vueuse/core": "^10.5.0",
    "@vueuse/integrations": "^10.5.0",
    "mammoth": "^1.9.1",
    "sortablejs": "^1.15.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/sortablejs": "^1.15.4",
    "@types/uuid": "^9.0.6",
    "@vitejs/plugin-vue": "^4.4.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^3.6.3",
    "vue": "^3.3.8",
    "vue-tsc": "^1.8.22"
  }
}
